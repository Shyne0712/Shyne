#include <stdio.h>

// Hàm kiểm tra xem số có phải là bội của 7 không
int isMultipleOf7(int num) {
    return (num % 7 == 0);
}

int main() {
    printf("Các số nguyên có 2 chữ số và là bội của 7:\n");

    for (int i = 10; i < 100; i++) {
        if (isMultipleOf7(i)) {
            printf("%d ", i);
        }
    }

    return 0;
}
